@{ 
    ViewData["Title"] = "Sign up";
   <h2 span style = "margin: 15px; margin-left:0">Sign up result</h2>
   <div span style = "margin-left:150px;">@Model.FName</div>
   <div span style = "margin-left:150px;">@Model.LName</div>
   <div span style = "margin-left:150px;">@Model.BDay @Model.BMonth @Model.BYear</div>
   <div span style = "margin-left:150px;">@Model.Gender</div>
   <div span style = "margin-left:150px;">@Model.EMail</div>
   <div span style = "margin-left:150px;">@Model.Password</div>
}
